{
  "name": "y",
  "version": "y",
  "description": "frontend for car pooling app",
  "main": "y",
  "repository": "https://github.com/mkarara91/carsharing-ui.git",
  "author": "Mohamed Karara",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open --color --progress --config webpack.dev.js",
    "build:prod": "webpack --color --progress --config webpack.prod.js",
    "build:dev": "webpack --color --progress --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier-eslint --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "styleCheck": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint",
      "prettier --check",
      "git add"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "google-map-react": "^2.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-places-autocomplete": "^3.3.2",
    "react-redux": "^7.2.5"
  },
  "devDependencies": {
    "path": "^0.12.7",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.3.2",
    "webpack": "^5.56.0",
    "webpack-dev-server": "^4.3.0",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.3.0",
    "enzyme": "^3.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "husky": "^7.0.2",
    "image-webpack-loader": "^8.0.1",
    "jest": "^27.2.4",
    "jest-scss-transform": "^1.0.1",
    "lint-staged": "^11.2.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "trim-newlines": "^4.0.2",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  }
}
